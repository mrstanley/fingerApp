{
    "extends": "tslint:latest",
    "rulesDirectory": ["app/"],
    "rules": {
        "no-var-requires": false,
        "no-empty": false,
        "no-unused-expression": false,
        "no-shadowed-variable": false,
        "no-bitwise": false,
        "variable-name": [
            false
        ],
        "curly": false,
        // "member-access": [
        //     false
        // ],
        "one-variable-per-declaration": [
            false
        ],
        "indent": [

        ],
        "object-literal-sort-keys": false,
        "max-line-length": [

        ],
        "no-console": [
            false
        ],
        "trailing-comma": [true, {
            "multiline": "never",
            "singleline": "never"
        }]
    }
}